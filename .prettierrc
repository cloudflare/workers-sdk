{
	"useTabs": true,
	"singleQuote": true,
	"plugins": [
		"@ianvs/prettier-plugin-sort-imports",
		"prettier-plugin-packagejson"
	],
	"importOrder": [
		"<BUILTIN_MODULES>",
		"<THIRD_PARTY_MODULES>",
		"^[.]{2}$",
		"^[.]{2}/",
		"^[.]/(?!index)",
		"^[.]$",
		"^[.]/index$",
		"<TYPES>"
	],
	"importOrderTypeScriptVersion": "5.5.2",
	"importOrderParserPlugins": [
		"typescript",
		"jsx",
		"decorators",
		"explicitResourceManagement"
	],
	"overrides": [
		{
			"files": "*.d.ts",
			"options": {
				"importOrderParserPlugins": [
					"[\"typescript\", { \"dts\": true }]",
					"decorators"
				]
			}
		}
	]
}
