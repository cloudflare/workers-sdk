{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"strict": true,
		"declaration": true,
		"resolveJsonModule": true,
		"types": ["node-unenv"]
	},
	"include": ["**/*.ts"],
	"exclude": ["/dist"]
}
