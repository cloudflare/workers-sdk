{
	"name": "@cloudflare/local-explorer-ui",
	"version": "0.3.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "openapi-ts && vite build",
		"check:lint": "eslint src --max-warnings=0",
		"check:type": "tsc --build",
		"deploy": "echo 'no deploy'",
		"predev": "pnpm -w build --filter wrangler",
		"dev": "concurrently -n api,ui,worker -c green,blue,orange \"openapi-ts --watch\" \"vite\" \"pnpm --filter @fixture/worker-with-resources start\"",
		"dev:ui": "concurrently -n api,ui -c green,blue \"openapi-ts --watch\" \"vite\"",
		"postinstall": "openapi-ts",
		"preview": "vite preview",
		"test": "vitest",
		"test:ci": "vitest run",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@base-ui/react": "^1.1.0",
		"@cloudflare/kumo": "^1.5.0",
		"@cloudflare/style-const": "^6.1.3",
		"@cloudflare/util-http": "^2.0.18",
		"@cloudflare/util-routes": "^5.3.0",
		"@cloudflare/workers-editor-shared": "^0.1.1",
		"@codemirror/autocomplete": "^6.18.0",
		"@codemirror/commands": "^6.6.0",
		"@codemirror/lang-sql": "^6.7.0",
		"@codemirror/language": "^6.10.0",
		"@codemirror/state": "^6.4.0",
		"@codemirror/view": "^6.33.0",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@floating-ui/react": "^0.26.3",
		"@lezer/common": "^1.2.0",
		"@lezer/highlight": "^1.2.0",
		"@phosphor-icons/react": "^2.1.10",
		"@tailwindcss/vite": "^4.0.15",
		"@tanstack/react-router": "^1.158.0",
		"immer": "^10.0.0",
		"lodash": "^4.17.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-split-pane": "^0.1.92",
		"tailwindcss": "^4.0.15"
	},
	"devDependencies": {
		"@cloudflare/eslint-config-shared": "workspace:*",
		"@cloudflare/workers-tsconfig": "workspace:*",
		"@hey-api/openapi-ts": "^0.91.1",
		"@tanstack/react-router-devtools": "^1.158.0",
		"@tanstack/router-plugin": "^1.158.0",
		"@types/lodash": "^4.17.0",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"@vitejs/plugin-react": "^4.4.1",
		"concurrently": "^9.0.0",
		"typescript": "catalog:default",
		"vite": "catalog:default",
		"vite-plugin-svgr": "^4.3.0",
		"vitest": "catalog:default"
	}
}
