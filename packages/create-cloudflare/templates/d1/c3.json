{
	"id": "d1",
	"displayName": "Worker with D1",
	"platform": "workers",
	"languages": ["js", "ts"],
	"copyFiles": {
		"js": {
			"path": "./js"
		},
		"ts": {
			"path": "./ts"
		}
	},
	"bindings": {
		"d1Databases": [
			{
				"boundVariable": "DB",
				"defaultValue": "my-queue",
				"seedFiles": [
					{ "path": "./data/schema.sql" },
					{ "path": "./data/data.sql" }
				]
			}
		]
	}
}
