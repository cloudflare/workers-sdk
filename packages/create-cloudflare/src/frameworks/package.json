{
	"name": "frameworks_clis_info",
	"description": [
		"this package.json is only used to keep track of the frameworks cli dependencies",
		"so that we can use dependabot to update these dependencies automatically",
		"additionally it also contains a map that maps frameworks to their respective clis"
	],
	"dependencies": {
		"create-astro": "4.8.0",
		"create-analog": "1.3.1",
		"@angular/create": "18.1.4",
		"create-docusaurus": "3.5.2",
		"create-hono": "0.12.0",
		"create-next-app": "14.2.5",
		"create-qwik": "1.5.7",
		"create-vite": "5.2.3",
		"create-remix": "2.11.1",
		"create-solid": "0.5.12",
		"create-svelte": "6.3.8",
		"create-vue": "3.10.4",
		"gatsby": "5.13.7",
		"nuxi": "3.12.0"
	},
	"frameworkCliMap": {
		"analog": "create-analog",
		"angular": "@angular/create",
		"astro": "create-astro",
		"docusaurus": "create-docusaurus",
		"gatsby": "gatsby",
		"hono": "create-hono",
		"next": "create-next-app",
		"nuxt": "nuxi",
		"qwik": "create-qwik",
		"react": "create-vite",
		"remix": "create-remix",
		"solid": "create-solid",
		"svelte": "create-svelte",
		"vue": "create-vue"
	}
}
