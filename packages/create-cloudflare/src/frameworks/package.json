{
	"name": "frameworks_clis_info",
	"description": [
		"this package.json is only used to keep track of the frameworks cli dependencies",
		"so that we can use dependabot to update these dependencies automatically",
		"additionally it also contains a map that maps frameworks to their respective clis"
	],
	"dependencies": {
		"create-astro": "4.7.5",
		"create-analog": "1.2.0",
		"@angular/create": "17.2.3",
		"create-docusaurus": "3.1.1",
		"create-hono": "0.7.0",
		"create-next-app": "14.1.0",
		"create-qwik": "1.4.5",
		"create-react-app": "5.0.1",
		"create-remix": "2.8.1",
		"create-solid": "0.5.5",
		"create-svelte": "6.0.10",
		"create-vue": "3.10.2",
		"gatsby": "5.13.3",
		"nuxi": "3.11.1"
	},
	"frameworkCliMap": {
		"analog": "create-analog",
		"angular": "@angular/create",
		"astro": "create-astro",
		"docusaurus": "create-docusaurus",
		"gatsby": "gatsby",
		"hono": "create-hono",
		"next": "create-next-app",
		"nuxt": "nuxi",
		"qwik": "create-qwik",
		"react": "create-react-app",
		"remix": "create-remix",
		"solid": "create-solid",
		"svelte": "create-svelte",
		"vue": "create-vue"
	}
}
