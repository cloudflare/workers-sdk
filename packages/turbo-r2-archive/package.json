{
	"name": "@cloudflare/turbo-r2-archive",
	"version": "0.0.4",
	"private": true,
	"description": "TurboRepo API Compliant Remote Caching w/ Cloudflare Workers using R2",
	"keywords": [
		"workers",
		"cloudflare",
		"typescript",
		"turborepo",
		"remote cache"
	],
	"scripts": {
		"build": "wrangler deploy --dry-run --outdir=./dist",
		"check:lint": "eslint .  --max-warnings=0",
		"deploy": "wrangler deploy",
		"start": "wrangler dev",
		"type:check": "tsc"
	},
	"dependencies": {
		"@hono/zod-validator": "^0.7.5",
		"hono": "^4.7.0",
		"zod": "^3.25.0 || ^4.0.0"
	},
	"devDependencies": {
		"@cloudflare/eslint-config-shared": "workspace:*",
		"@cloudflare/workers-tsconfig": "workspace:*",
		"eslint": "catalog:default",
		"wrangler": "workspace:*"
	},
	"peerDependencies": {
		"zod": "^3.25.0 || ^4.0.0"
	},
	"volta": {
		"extends": "../../package.json"
	},
	"workers-sdk": {
		"deploy": true
	}
}
