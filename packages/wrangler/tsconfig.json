{
	"extends": "@cloudflare/workers-tsconfig/tsconfig.json",
	"compilerOptions": {
		"module": "preserve",
		"types": ["node"],
		"jsx": "react-jsx",
		"tsBuildInfoFile": ".tsbuildinfo"
	},
	"include": ["**/*.ts", "**/*.tsx", "**/*.js"],
	"exclude": [
		"emitted-types",
		"node_modules",
		"*-dist",
		"src/miniflare-cli",
		"templates",
		"**/__tests__",
		"**/*.test.ts",
		"kv-asset-handler.js",
		"e2e"
	]
}
