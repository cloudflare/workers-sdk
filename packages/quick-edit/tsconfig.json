{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021"],
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": ["web", "editor-files"]
}
