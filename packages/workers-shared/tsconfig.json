{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021"],
		"module": "es2022",
		"moduleResolution": "Bundler",
		"types": ["@cloudflare/workers-types/experimental", "@types/node"],
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["utils/*.ts", "index.ts"],
	"exclude": ["scripts/**"]
}
