{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021"],
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"types": ["@cloudflare/workers-types/experimental", "@types/node"],
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
