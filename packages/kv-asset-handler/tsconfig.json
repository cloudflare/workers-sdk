{
	"compilerOptions": {
		"outDir": "./dist",
		"noImplicitAny": true,
		"target": "ES2022",
		"allowJs": false,
		"types": ["@cloudflare/workers-types"],
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"skipLibCheck": true
	},
	"include": [
		"./src/*.ts",
		"./src/**/*.ts",
		"./test/**/*.ts",
		"./test/*.ts",
		"./src/types.d.ts"
	],
	"exclude": ["node_modules/", "dist/"]
}
