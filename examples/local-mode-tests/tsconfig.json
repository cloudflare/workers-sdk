{
  "compilerOptions": {
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "esnext",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}
