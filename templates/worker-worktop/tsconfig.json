{
	"compilerOptions": {
		"module": "esnext",
		"target": "es2022",
		"types": ["@cloudflare/workers-types"],
		"moduleResolution": "node",
		"noEmit": true,
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": ["build", "node_modules"]
}
