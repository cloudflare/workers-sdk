{
	"name": "remix-pages-app",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix build",
		"check:type": "tsc",
		"dev": "remix dev --manual -c \"npm run start\"",
		"start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
