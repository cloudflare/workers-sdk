{
	"compilerOptions": {
		"isolatedModules": true,
		"esModuleInterop": true,
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"target": "esnext",
		"strict": true,
		"noEmit": true,
		"types": ["@cloudflare/workers-types", "node"],
		"lib": ["esnext"],
		"skipLibCheck": true
	},
	"include": ["**/*.ts"],
	"exclude": ["tests"]
}
