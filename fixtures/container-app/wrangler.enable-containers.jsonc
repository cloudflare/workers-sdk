{
	"name": "container-app",
	"main": "src/index.ts",
	"compatibility_date": "2025-04-03",
	"dev": { "enable_containers": false },
	"containers": [
		{
			"configuration": {
				"image": "./Dockerfile",
			},
			"class_name": "Container",
			"name": "http2",
			"max_instances": 2,
		},
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "Container",
				"name": "CONTAINER",
			},
		],
	},
	"migrations": [
		{
			"tag": "v1",
			"new_classes": ["Container"],
		},
	],
}
