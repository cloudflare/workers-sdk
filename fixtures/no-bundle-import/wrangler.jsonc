{
	"name": "no-bundle-import",
	"main": "src/index.js",
	"compatibility_date": "2023-02-20",
	"compatibility_flags": [
		"fetch_iterable_type_support",
		"fetch_iterable_type_support_override_adjustment",
		"enable_nodejs_process_v2",
	],
	"no_bundle": true,
	"rules": [
		{
			"type": "CommonJS",
			"globs": ["nested/say-hello.js", "**/*.cjs"],
		},
		{
			"type": "ESModule",
			"globs": ["**/*.js"],
		},
		{
			"type": "Text",
			"globs": ["**/*.json"],
			"fallthrough": true,
		},
	],
}
