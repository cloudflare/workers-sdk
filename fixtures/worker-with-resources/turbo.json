{
	"extends": ["//"],
	"tasks": {
		"start": {
			"dependsOn": ["wrangler#build"],
			// We don't want to cache the (empty) output of this dev server task or it might not start again (just saying "Full Turbo")
			"cache": false,
			// This task starts a long running dev server that is not expected to exit automatically
			"persistent": true
		}
	}
}
