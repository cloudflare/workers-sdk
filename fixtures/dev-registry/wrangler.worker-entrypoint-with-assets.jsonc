{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "worker-entrypoint-with-assets",
	"main": "./workers/worker-entrypoint.ts",
	"compatibility_date": "2025-05-01",
	"assets": {
		"directory": "./assets",
	},
	"services": [
		{
			"binding": "SERVICE_WORKER",
			"service": "service-worker",
		},
		{
			"binding": "MODULE_WORKER",
			"service": "module-worker",
		},
		{
			"binding": "MODULE_WORKER_WITH_ASSETS",
			"service": "module-worker-with-assets",
		},
		{
			"binding": "WORKER_ENTRYPOINT",
			"service": "worker-entrypoint",
		},
		{
			"binding": "NAMED_ENTRYPOINT",
			"service": "worker-entrypoint",
			"entrypoint": "NamedEntrypoint",
		},
		{
			"binding": "NAMED_MODULE_WORKER",
			"service": "module-worker",
			"entrypoint": "NamedEntrypoint",
		},
		{
			"binding": "NAMED_MODULE_WORKER_WITH_ASSETS",
			"service": "module-worker-with-assets",
			"entrypoint": "NamedEntrypoint",
		},
	],
	"tail_consumers": [
		{
			"service": "module-worker",
		},
	],
}
