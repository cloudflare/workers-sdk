{
	"$schema": "../../node_modules/wrangler/config-schema.json",
	"name": "main-worker",
	"main": "./index.ts",
	"compatibility_date": "2025-11-01",
	"compatibility_flags": ["enable_ctx_exports"],
	"vars": {
		"NAME": "MainWorker",
	},
	"services": [
