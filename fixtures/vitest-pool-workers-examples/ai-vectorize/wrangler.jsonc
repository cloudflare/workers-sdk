{
	"name": "ai-vectorize",
	"main": "src/index.ts",
	// compatibility_date is required so we can run `wrangler build`
	"compatibility_date": "2025-03-20",
	"compatibility_flags": [
		"fetch_iterable_type_support",
		"fetch_iterable_type_support_override_adjustment",
		"enable_nodejs_process_v2",
	],
	"ai": {
		"binding": "AI",
	},
	"vectorize": [
		{
			"binding": "VECTORIZE",
			"index_name": "embeddings",
		},
	],
}
