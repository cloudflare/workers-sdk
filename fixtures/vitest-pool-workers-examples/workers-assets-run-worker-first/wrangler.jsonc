{
	"compatibility_flags": [
		"fetch_iterable_type_support",
		"fetch_iterable_type_support_override_adjustment",
		"enable_nodejs_process_v2",
	],
	"name": "workers-static-assets-with-user-worker-run-first",
	"main": "src/index.ts",
	// don't provide compatibility_date so that vitest will infer the latest one
	"assets": {
		"directory": "./public",
		"binding": "ASSETS",
		"html_handling": "auto-trailing-slash",
		"not_found_handling": "none",
		"run_worker_first": true,
	},
}
