{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noEmit": true,
    "lib": ["esnext"],
    "jsx": "react",
    "resolveJsonModule": true
  },
  "exclude": ["node_modules/", "packages/wrangler/vendor"]
}
