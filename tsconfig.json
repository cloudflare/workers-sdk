{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noEmit": true,
    "lib": ["esnext"],
    "jsx": "react",
    "resolveJsonModule": true,
    "incremental": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules/",
    "packages/*/vendor",
    "packages/*/*-dist",
    "packages/wrangler/pages/functions/template-worker.ts"
  ]
}
